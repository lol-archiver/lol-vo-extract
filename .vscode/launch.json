{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Extract audio and voice",
			"console": "integratedTerminal",
			"skipFiles": [
				"<node_internals>/**"
			],
			"runtimeExecutable": "node.exe",
			"program": "${workspaceFolder}/index.js",
		},
		{
			"type": "node",
			"request": "launch",
			"name": "0: Launch Current File",
			"console": "integratedTerminal",
			"skipFiles": [
				"<node_internals>/**"
			],
			"runtimeExecutable": "node.exe",
			"cwd": "${workspaceRoot}",
			"program": "${fileDirname}/${fileBasename}",
			"args": [
				"${workspaceRoot}"
			],
		},
		{
			"type": "node",
			"request": "launch",
			"name": "1: Concat audio every 50",
			"console": "integratedTerminal",
			"skipFiles": [
				"<node_internals>/**"
			],
			"runtimeExecutable": "node.exe",
			"program": "${workspaceFolder}/side/1-concat-audio-50.js",
		},
		{
			"type": "node",
			"request": "launch",
			"name": "2: Combine raw Lines",
			"console": "integratedTerminal",
			"skipFiles": [
				"<node_internals>/**"
			],
			"runtimeExecutable": "node.exe",
			"program": "${workspaceFolder}/side/2-concat-raw-line.js",
		},
		{
			"type": "node",
			"request": "launch",
			"name": "3: Update base data",
			"console": "integratedTerminal",
			"skipFiles": [
				"<node_internals>/**"
			],
			"runtimeExecutable": "node.exe",
			"program": "${workspaceFolder}/side/3-convert-base.js",
		},
		{
			"type": "node",
			"request": "launch",
			"name": "4: Copy audio with line",
			"console": "integratedTerminal",
			"skipFiles": [
				"<node_internals>/**"
			],
			"runtimeExecutable": "node.exe",
			"program": "${workspaceFolder}/side/4-copy-audio-with-line.js",
		},
		{
			"type": "node",
			"request": "launch",
			"name": "5: Convert rst",
			"console": "integratedTerminal",
			"skipFiles": [
				"<node_internals>/**"
			],
			"runtimeExecutable": "node.exe",
			"program": "${workspaceFolder}/side/5-convert-rst.js",
		},
		{
			"type": "node",
			"request": "launch",
			"name": "6: Update rst",
			"console": "integratedTerminal",
			"skipFiles": [
				"<node_internals>/**"
			],
			"runtimeExecutable": "node.exe",
			"program": "${workspaceFolder}/side/6-update-rst.js",
		},
	]
}
